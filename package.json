{
    "name": "vscode-simple-sync",
    "displayName": "Simple Sync",
    "description": "Little extension that help you sync your Code settings cross machines",
    "version": "0.0.1",
    "publisher": "living42",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
